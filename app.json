{
  "expo": {
    "name": "uptime",
    "slug": "alerto",
    "owner": "capstone_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "platforms": ["ios", "android"],
    "backgroundFetch": {
      "enabled": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/c4ff40b2-f5d0-47cf-83a7-b5ede873c4c6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jasonjimenez.uptime",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"],
        "UIBackgroundFetchInterval": 60
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#101113"
      },
      "useNextNotificationsApi": true,
      "package": "com.jasonjimenez.uptime"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c4ff40b2-f5d0-47cf-83a7-b5ede873c4c6"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
